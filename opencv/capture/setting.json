{
    "video_id": 0,
    "description": "will use /dev/video${video_id}",
    "default_width": 320,
    "default_height": 240,

    "use_realsense": true,
    "use_edge_test": true,
    "use_hsv_test": false,

    "output_dir": "outdir",

    "realsense": {
        "show_dist": false,
        "show_fps": true,
        "color_scheme": 2,
        "distance_limit": 0.7,
        "apply_sleep": false,
        "apply_align": true,
        "apply_disparity": true,
        "apply_dec": true,
        "apply_spatial": true,
        "apply_temporal": true,
        "apply_holefill": true
    }
}
